//U765139D JOB AT,'DISAS',MSGCLASS=X,NOTIFY=U765139,CLASS=A
//*
//*          DISASSEMBLE LOAD MODULE CONTAINING SYM RECORDS
//*
//     SET   INPUT=FDDSTO7.GEN.LOAD     <=== INPUT DATASET
//     SET   LMOD=DISAS                 <=== LMOD WITH SYM RECORDS
//*
//DISYM   EXEC PGM=DISYM
//STEPLIB  DD DSN=FDDSTO7.DISAS.LOAD,DISP=SHR
//SYSLIN   DD DSN=&INPUT(&LMOD),DISP=SHR
//SYSPUNCH DD DSN=&OBJECT,DISP=(,PASS),UNIT=SYSDA,SPACE=(TRK,(1,10))
//PRDD     DD SYSOUT=1
//PUDD     DD SYSOUT=2
//DADD     DD SYSOUT=3
//*
//GENCARD EXEC PGM=GENCARD,PARM='MEMBER(&LMOD).'
//STEPLIB  DD DSN=FDDSTO7.DISAS.LOAD,DISP=SHR
//SYSPUNCH DD DSN=&CARD,UNIT=VIO,SPACE=(TRK,1),DISP=(,PASS)
//*
//DELINKI EXEC PGM=DELINKI
//STEPLIB  DD DSN=FDDSTO7.DISAS.LOAD,DISP=SHR <=== DISAS, DELINKI ALSO
//*        DD DSN=CEE.SCEERUN,DISP=SHR  <=== IF NOT IN LINKLIST
//SYSLIB   DD DISP=SHR,DSN=&INPUT
//SYSLIN   DD DSN=&OBJECT,DISP=(MOD,PASS)
//SYSPRINT DD SYSOUT=0
//LINKCNTL DD DUMMY
//SYSIN    DD DSN=&CARD,DISP=(OLD,DELETE)
//*
//DISAS  EXEC PGM=DISAS
//STEPLIB DD DSN=FDDSTO7.DISAS.LOAD,DISP=SHR
//RDDD    DD DSN=&OBJECT,DISP=(OLD,DELETE)
//PRDD    DD SYSOUT=1
//PUDD    DD SYSOUT=2
//DADD    DD SYSOUT=3
//OUT     DD SYSOUT=4
//
